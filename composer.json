{
    "name": "mncee/salesforce-sync",
    "description": "Package for syncing Salesforce objects with local data",
    "keywords": [
        "laravel",
        "salesforce",
        "force.com toolkit",
        "laravel 5"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "John Wilson",
            "email": "jwilson@mncee.org"
        }
    ],
    "autoload": {
        "psr-4": {
            "CEE\\Salesforce\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "require": {
        "laravel/framework": "^9.0|^10.0|^11.0",
        "ext-soap": "*"
    },
    "extra": {
        "laravel": {
            "providers": [
                "CEE\\Salesforce\\Laravel\\SalesforceServiceProvider"
            ],
            "aliases": {
                "Salesforce": "CEE\\Salesforce\\Laravel\\Facades\\Salesforce"
            }
        }
    }
}
